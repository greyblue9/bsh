public static String classNamesToSql(String regex) { HashMap cmb = new HashMap(); StringBuilder sb = new StringBuilder(76 * 100); for (String n: searchClassPath(regex)){  caps = n.charAt(0) + n.replaceAll("[^A-Z]+", "").toLowerCase(); rn = n; if (caps.length() == 1 &&  n.length() >= 2) { caps = caps + String.valueOf(n.charAt(1)).toLowerCase(); } int l = 2; lc = caps; cmb.put(caps, rn); if (String.valueOf(caps.charAt(0)).toUpperCase().equals(String.valueOf(caps.charAt(0)))) caps = caps.substring(1); sb.append(String.format("insert into utext (title, note) values (\"%s\", \"%s\"); \n", caps, rn.replaceAll("\"", "\\\""))); } sql = sb.toString().replaceAll("\\(\"\"", "(\"\"\"").replaceAll("\\.invoke\\(", "(").replaceAll("\"bbsh", "bb");  return sql; }