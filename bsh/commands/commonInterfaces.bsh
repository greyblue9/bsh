
public static void commonInterfaces(String ifsearch, String tgtsearch) { Class c; int total; total = 0; Set cset = new HashSet();  for (Class cls: searchClassPath(null, ifsearch)) { if (cls.isInterface()) cset.add(cls); Collections.addAll(cset, cls.getInterfaces()); }  for (tgtcls: cset) { StringBuilder sb = new StringBuilder(76); for (c: searchClassPath(null, tgtsearch)) { if (! tgtcls.isAssignableFrom(c)) continue;  sb.append(String.format("\n    %c[%sm%s <: %s%c[0m ?  -->  %s", 0x1b, tgtcls.isAssignableFrom(c)? "1;32": "1;30", tgtcls.getSimpleName(), c.getSimpleName(), 0x1b, Boolean.valueOf((ok = tgtcls.isAssignableFrom(c))))); if (ok) total += 1; } if (total == 0) continue;  sb.insert(0, String.format("\n\n%s  total:  %c[1;36m%d%c[0m", dumpMembers.colorize(ReflectionUtil.getClassName(tgtcls), "1;33"), 0x1b, total, 0x1b)); System.out.println(sb.toString()); total = 0; } }

