public static byte[] dumpEval(String evalStr) { Object evalObj; byte[] bufbytes; PrintStream out = System.out; PrintStream bufps; try { System.setOut(bufps = new PrintStream (baos = new ByteArrayOutputStream())); this.interpreter.eval(evalStr); bufps.flush(); baos.flush(); bufps.close(); baos.close(); bufbytes = baos.toByteArray(); } finally { System.setOut(out); System.out.println("done"); } System.out.println(hexDump(bufbytes)); return bufbytes; }

public static byte[] dumpEval(String evalStr, boolean stderr) { Object evalObj; byte[] bufbytes; PrintStream err = System.err; PrintStream bufps; try { System.setErr(bufps = new PrintStream (baos = new ByteArrayOutputStream())); this.interpreter.eval(evalStr); bufps.flush(); baos.flush(); bufps.close(); baos.close(); bufbytes = baos.toByteArray(); } finally { System.setErr(err); System.out.println("done"); } System.out.println(hexDump(bufbytes)); return bufbytes; }