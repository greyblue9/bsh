public static Object findClassSource(String className) { String classNameAsPath = ClassInfo.classNameToPath(className, "class"); List<URL> urls = Arrays.asList(CollectionUtil.toArray(ClassLoader.getBootstrapResources(classNameAsPath))); Class cls = dumpMembers.getClass(className); if (cls.getClassLoader() == null) { return CollectionUtil.firstOrDefault(urls); } else { return cls.getClassLoader().getResource(classNameAsPath); }; };