public static Class[] findRealClasses(Object arg) { return ClassPathUtil.searchClassPath(Pattern.compile(String.valueOf(arg), Pattern.CASE_INSENSITIVE)).to(Class).select(FunctionUtil.eval("""return m -> { (m.getModifiers() & Modifier.PUBLIC) == Modifier.PUBLIC && (m.getModifiers() & Modifier.ABSTRACT) == 0 && m.isInterface() == false && m.isEnum() == false && (m != Object.class && m.getSuperclass().getSimpleName().endsWith("isitor") == false)? m: null; }""")); }