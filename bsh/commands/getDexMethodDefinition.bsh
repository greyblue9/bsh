public static MethodDefinition getDexMethodDefinition(Member mtd) { Class cls = mtd.getDeclaringClass(); Pair<TypeDefinition_ByteArr> result = ProcyonUtil.getDexTypeDefinition(org.d6r.ClassInfo.getDex(cls).getBytes(), cls.getName(), null); TypeDefinition td = result.getKey(); MethodDefinition md = ProcyonUtil.findMatchingMethodDefinition(td, (mtd instanceof Method)? ((Method)mtd).getName(): "<init>", ProcyonUtil.getRawMethodSig(mtd)); return md; };