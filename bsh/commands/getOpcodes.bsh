public static String[] getOpcodes() { String[] opcodes = new String[202]; for (Field opcField: StringCollectionUtil.toStringFilter(javassist.bytecode.Opcode.class.getDeclaredFields(), "public static final int (?:[^.]+\\.)*[A-Z0-9_]+")) { int opcval = opcField.get(null); if (opcval >= opcodes.length) { String[] new_opcodes = new String[255]; System.arraycopy(opcodes, 0, new_opcodes, 0, opcodes.length); opcodes = new_opcodes; } opcodes[opcval] = opcField.getName().toLowerCase(); } }