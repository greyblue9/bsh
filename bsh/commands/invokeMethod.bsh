

public static Object invokeMethod
(Object obj, String name, Object[] args) 
{
   Method m = findMethod(obj, name); 
   if (m == null) return Void.TYPE; 
   m.invoke(obj, args); 
   rs = $_; 
   if (m.getReturnType().equals(Void.TYPE)) {
     unset("$_"); 
     return m.getReturnType();
   }  
   return rs;
}



