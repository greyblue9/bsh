Class loadClass(java.lang.String name) { java.lang.Class cls = null; try { cls = Thread.currentThread().getContextClassLoader().loadClass(name); } catch (java.lang.Exception e) { return null; }; if (cls==null) return null; cm = this.caller.namespace.getClassManager(); cm.cacheClassInfo(name, cls); if (name.indexOf(".") != -1) cm.cacheClassInfo(name.substring(name.lastIndexOf(".")+1), cls); java.lang.reflect.Field f = bsh.BshClassManager.class.getDeclaredField("declaringInterpreter"); f.setAccessible(true);  i = f.get(cm); i.eval("import "+cls.getName()+";"); return cls; };
