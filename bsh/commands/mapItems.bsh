public static void mapItems(Map x) { List a = new ArrayList(); for (Map.Entry e: x.entrySet()) { a.add(String.format("%20s = %s", e.getKey(), (e.getValue().getValue() instanceof Iterable || e.getValue().getValue() instanceof Map) ? String.format("%s (size = %d)", new bsh.ClassIdentifier(e.getValue().getValue().getClass()).toString(), e.getValue().getValue().size()): String.format("%s", (e.getValue().getValue() != null) ? new bsh.ClassIdentifier(e.getValue().getValue().getClass()).toString() : "\u001b[1;30m<null>\u001b[0m")));  }; System.out.println(StringUtils.join(a, "\n")); };
