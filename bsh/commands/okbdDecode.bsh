public static String okbdDecode(String url) { import java.util.regex.*; Pattern ptrn = Pattern.compile("^\\s*https?://(?:[^/]*([^/]*ling|honso)[^/]*)/([^?&#]*)\\?(.*)$"); Matcher mr = ptrn.matcher(url); if (mr.find(0)) { String qstr = mr.group(3);  q = org.apache.commons.httpclient.util.URIUtil.decode(qstr.replaceAll("(^\\w)", "%20").replaceAll("%([^0-9a-fA-F].)", "%25$1").replaceAll("%(.[^0-9a-fA-F])", "%25$1"));  String unesc = qstr.replaceAll("%0d", "\n").replaceAll("(M-\\^?.){1,6}", " "); return unesc; } return null; }
