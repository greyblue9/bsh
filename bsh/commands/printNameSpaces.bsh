import java.util.Map.Entry;
public static Entry[] printNameSpaces() { Comparator cmp = new Comparator() { public int compare(Object o1, Object o2) { String name1 = ((BshBinding) ((Entry) o1).getValue()).getName();   String name2 = ((BshBinding) ((Entry) o2).getValue()).getName();   return Integer.compare(name1.length(), name2.length()); } };        ents = CollectionUtil.toArray(BlockNameSpaceFactory.INSTANCE.nsMap.entrySet().sort()); Arrays.sort(ents, cmp); for (int i=0; i<ents.length; i++) { ent = ents[i]; ns = ent.getValue(); System.out.println(ns.getName()); System.out.printf("\n  vars:   %s\n  parent: %s\n  inst:   %s\n  node:   %s\n\n", ns.variables, ns.getParent() != null? ns.getParent().getName(): "null", ns.id, Reflector.invokeOrDefault(ns, "getNode")); }; return ents; }

