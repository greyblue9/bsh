Object obj = this.namespace.getClassManager().getClassPath(); Class cls = obj.getClass(); import com.fasterxml.classmate.*; import com.fasterxml.classmate.members.*; TypeResolver tr = new TypeResolver(); ResolvedType rt = tr.resolve(cls); mr = new MemberResolver(tr); ResolvedTypeWithMembers rtm = mr.resolve(rt,null,null); ResolvedField[] rfs = rtm.getMemberFields(); for (ResolvedField rf: rfs) { sig = rf.getType().getFullDescription(); sig = sig.replaceAll("(.+?)( (extends|super).*)*$", "$1"); System.out.println(dumpMembers.colorize(sig, "1;36") + " " + rf.getName()); }
