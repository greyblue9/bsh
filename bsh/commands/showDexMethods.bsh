public static Iterable<Method> showDexMethods(Object methodGroup) { Iterable overloadSet = (methodGroup instanceof Iterable? (Iterable) methodGroup: Arrays.asList(methodGroup)); System.out.printf("\nDexMethods:\n  %s\n\n", StringUtils.join(overloadSet.select(FunctionUtil.bind("""return m -> { var di = DebugReader.getDebugInfo(m); Pair.of(m, String.format("%c    line %d - %d%c    %s", 0x0a, di.lineStart, di.lineEnd, 0x0a, di.sourceName)); }""")).evalSelect("""m => StringUtils.join(Arrays.asList(CollectionUtil.firstOrDefault(findDexMethod(m.getKey())), m.getValue()), " ");"""),"\n\n  ")); return overloadSet; };